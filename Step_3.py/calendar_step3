def MyCalender():


 x = """ Day 	Time 		   Subject
Monday	  9:10-10:15         Band
	      10:15-10:35        Break
          10:35-11:40        P.E
        
Tuesday 9:10-10:15         Math
        10:15-10:35        Break
        10:35-11:40        Language Arts"""
        
 return x

mycalender = MyCalender()

if "Monday"  in mycalender:
  print ("Monday is present")
else:
    print ("Monday is not present")
        
print(mycalender.upper())       


     

